    },
    {
      "from": 221.109,
      "to": 223.88,
      "sid": 68,
      "location": 2,
      "content": "他在这个单片机当中啊",
      "music": 0.0
    },
    {
      "from": 224.179,
      "to": 227.179,
      "sid": 69,
      "location": 2,
      "content": "通常是和左相环电路配合使用的",
      "music": 0.0
    },
    {
      "from": 227.179,
      "to": 231.14,
      "sid": 70,
      "location": 2,
      "content": "v提供系统所需要的一个时钟频率",
